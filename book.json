{
  "gitbook":   "3.2.2",
  "title": "Rasti",
  "description": "Minimalistic JavaScript MV library for building user interfaces.",
  "root":      ".",
  "structure": { 
    "readme":  "README.md",
    "summary": "SUMMARY.md" 
  },
  "author": "Alberto Masuelli <alberto.masuelli@gmail.com> (https://github.com/8tentaculos)",
  "plugins": [
    "-sharing", 
    "-livereload", 
    "-fontsettings", 
    "-search",
    "-lunr", 
    "toolbar", 
    "favicon"
  ],
  "pluginsConfig": {
    "toolbar": {
      "buttons": [
          {
            "label": "GitHub",
            "icon": "fa fa-github",
            "url": "https://github.com/8tentaculos/rasti"
          },
          {
            "label": "npm",
            "icon": "fa fa-bullseye",
            "url": "https://www.npmjs.com/package/rasti"
          }
      ]
    },
    "favicon": {
      "shortcut": "favicon.svg",
      "bookmark": "favicon.svg"
    }
  }
}